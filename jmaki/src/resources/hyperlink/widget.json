{
    "name": "hyperlink",
    "type": "@JS_NAME@",
    "version": "@JS_VERSION@",
    "jmakiVersion": "1.8",
    "description": "Woodstock hyperlink widget.",
    "args": [
      {"accessKey": {
         "type": "STRING",
         "description": "Accessibility access key.",
         "defaultValue": ""}},
      {"charset": {
         "type": "STRING",
         "description": "The character encoding of the URL.",
         "defaultValue": ""}},
      {"className": {
         "type": "STRING",
         "description": "CSS selector.",
         "defaultValue": ""}},
      {"dir": {
         "type": "STRING",
         "description": "Specifies the directionality of text.",
         "defaultValue": "ltr"}},
      {"disabled": {
         "type": "BOOLEAN",
         "description": "Disable the element.",
         "defaultValue": false}},
      {"formId": {
         "type": "STRING",
         "description": "The id of the HTML form element for page submit.",
         "defaultValue": ""}},
      {"lang": {
         "type": "STRING",
         "description": "Specifies the language of attribute values.",
         "defaultValue": ""}},
      {"name": {
         "type": "STRING",
         "description": "The anchor element unique identifer on the page.",
         "defaultValue": ""}},
      {"onBlur": {
         "type": "STRING",
         "description": "Element lost focus.",
         "defaultValue": ""}},
      {"onClick": {
         "type": "STRING",
         "description": "Mouse button is clicked on element.",
         "defaultValue": ""}},
      {"onDblClick": {
         "type": "STRING",
         "description": "Mouse button is double-clicked on element.",
         "defaultValue": ""}},
      {"onFocus": {
         "type": "STRING",
         "description": "Element received focus.",
         "defaultValue": ""}},
      {"onKeyDown": {
         "type": "STRING",
         "description": "Key is pressed down over element.",
         "defaultValue": ""}},
      {"onKeyPress": {
         "type": "STRING",
         "description": "Key is pressed and released over element.",
         "defaultValue": ""}},
      {"onKeyUp": {
         "type": "STRING",
         "description": "Key is released over element.",
         "defaultValue": ""}},
      {"onMouseDown": {
         "type": "STRING",
         "description": "Mouse button is pressed over element.",
         "defaultValue": ""}},
      {"onMouseOut": {
         "type": "STRING",
         "description": "Mouse is moved away from element.",
         "defaultValue": ""}},
      {"onMouseOver": {
         "type": "STRING",
         "description": "Mouse is moved onto element.",
         "defaultValue": ""}},
      {"onMouseUp": {
         "type": "STRING",
         "description": "Mouse button is released over element.",
         "defaultValue": ""}},
      {"onMouseMove": {
         "type": "STRING",
         "description": "Mouse is moved while over element.",
         "defaultValue": ""}},
      {"params": {
         "type": "ARRAY",
         "description": "The query parameters to be passed in the page submit.",
         "defaultValue": []}},
      {"style": {
         "type": "STRING",
         "description": "Specify style rules inline.",
         "defaultValue": ""}},
      {"tabIndex": {
         "type": "INT",
         "description": "Position in tabbing order.",
         "defaultValue": -1}},
      {"title": {
         "type": "STRING",
         "description": "Provides a title for element.",
         "defaultValue": "Hyperlink widget"}},
      {"visible": {
         "type": "BOOLEAN",
         "description": "Hide or show element.",
         "defaultValue": true}}
    ],
    "value": {
      "required": true,
      "type": "OBJECT",
      "defaultValue": {"formId": "form_identifier", "contents": [ "hyperlink text" ]},
      "description": "Simple hyperlink form submit with text."
    },
    "config": {
      "type": {
        "id": "@JS_NAME@",
        "libs": [
          "@JS_PATH@/bootstrap.js"
        ],
        "resources": [ "../resources/" ]
        }
    }
}
