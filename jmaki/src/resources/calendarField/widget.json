{
    "name": "calendarField",
    "type": "@JS_NAME@",
    "version": "@JS_VERSION@",
    "jmakiVersion": "1.8",
    "description": "Woodstock calendarField widget.",
    "args": [
      {"accesskey": {
         "type": "STRING",
         "description": "Shortcut key.",
         "defaultValue": ""}},
     {"align": {
        "type": "STRING",
        "description": "Alignment of image input.",
        "defaultValue": ""}},
      {"autoSave": {
        "type": "BOOLEAN",
        "description": "",
        "defaultValue": false}},
      {"className": {
         "type": "STRING",
         "description": "CSS selector.",
         "defaultValue": ""}},
      {"date": {
        "type": "STRING",
        "description": "An initial date to pre-set in the value.",
        "defaultValue": ""}},
      {"dir": {
         "type": "STRING",
         "description": "Specifies the directionality of text.",
         "defaultValue": "ltr"}},
      {"disabled": {
         "type": "BOOLEAN",
         "description": "Disable element.",
         "defaultValue": false}},
      {"label": {
         "type": "OBJECT",
         "description": "Define a lable widget properties.",
         "defaultValue": {"value": "Mylabel", "level": 2}}},
      {"lang": {
         "type": "STRING",
         "description": "Specifies the language of attribute values.",
         "defaultValue": ""}},
      {"maxLength": {
         "type": "INT",
         "description": "Maximum length allowed for input text, in characters.",
         "defaultValue": 30}},
      {"notify": {
         "type": "ARRAY",
         "description": "List of widget identifiers to notify for auto validation.",
         "defaultValue": []}},
      {"onBlur": {
         "type": "STRING",
         "description": "Element lost focus.",
         "defaultValue": ""}},
      {"onClick": {
         "type": "STRING",
         "description": "Mouse button is clicked on element.",
         "defaultValue": ""}},
      {"onDblClick": {
         "type": "STRING",
         "description": "Mouse button is double-clicked on element.",
         "defaultValue": ""}},
      {"onFocus": {
         "type": "STRING",
         "description": "Element received focus.",
         "defaultValue": ""}},
      {"onKeyDown": {
         "type": "STRING",
         "description": "Key is pressed down over element.",
         "defaultValue": ""}},
      {"onKeyPress": {
         "type": "STRING",
         "description": "Key is pressed and released over element.",
         "defaultValue": ""}},
      {"onKeyUp": {
         "type": "STRING",
         "description": "Key is released over element.",
         "defaultValue": ""}},
      {"onMouseDown": {
         "type": "STRING",
         "description": "Mouse button is pressed over element.",
         "defaultValue": ""}},
      {"onMouseOut": {
         "type": "STRING",
         "description": "Mouse is moved away from element.",
         "defaultValue": ""}},
      {"onMouseOver": {
         "type": "STRING",
         "description": "Mouse is moved onto element.",
         "defaultValue": ""}},
      {"onMouseUp": {
         "type": "STRING",
         "description": "Mouse button is released over element.",
         "defaultValue": ""}},
      {"onMouseMove": {
         "type": "STRING",
         "description": "Mouse is moved while over element.",
         "defaultValue": ""}},
      {"readOnly": {
         "type": "BOOLEAN",
         "description": "The text value cannot be entered of changed.",
         "defaultValue": false}},
      {"required": {
         "type": "BOOLEAN",
         "description": "A text value must be entered for this field.",
         "defaultValue": false}},
      {"size": {
         "type": "INT",
         "description": "The size of the displayed text, in characters.",
         "defaultValue": 30}},
      {"style": {
         "type": "STRING",
         "description": "Specify style rules inline.",
         "defaultValue": ""}},
      {"submitForm": {
         "type": "BOOLEAN",
         "description": "If true, the page form is submitted on return.",
         "defaultValue": false}},
      {"tabIndex": {
         "type": "INT",
         "description": "Position in tabbing order.",
         "defaultValue": -1}},
      {"title": {
         "type": "STRING",
         "description": "Provides a title for element.",
         "defaultValue": "CalendarField widget"}},
      {"valid": {
         "type": "BOOLEAN",
         "description": "If true, the text value is considered valid.",
         "defaultValue": true}},
      {"value": {
         "type": "STRING",
         "description": "Value of input.",
         "defaultValue": ""}},
      {"visible": {
         "type": "BOOLEAN",
         "description": "Hide or show element.",
         "defaultValue": true}}
    ],
    "value": {
      "required": true,
      "type": "OBJECT",
      "defaultValue": {label: {value: 'Date:'}, calendar: { dateFormat: 'MM/dd/yyyy'}},
      "description": "Simple calendar field with label and date format."
    },
    "config": {
      "type": {
        "id": "@JS_NAME@",
        "libs": [
          "@JS_PATH@/bootstrap.js"
        ],
        "resources": [ "../resources/" ]
        }
    }    
}
