{
   'name': 'textField',
   'type': '@JS_NAME@',
   'version': '@JS_VERSION@',
   'jmakiVersion': '1.0',
   'description': 'Woodstock text field widget',
   'value': {
      'required': true,
      'type': 'OBJECT',
      'defaultValue': {
         'value': '',
         'readOnly': false,
         'required': false,
         'submitForm': false,
         'autoValidate': false,
         'autoComplete': false,
	 'label': {'level': 2, 'value': 'Label'}
       }
    },
    'config': {
       'type': {
          'id': '@JS_NAME@.textField',
          'libs': [
             '@JS_PATH@/bootstrap.js'
             ],
          'preload': 'if (typeof @JS_NS@Config == \"undefined\") var @JS_NS@Config = {isDebug: true, parseOnLoad: false, namespace: \"woodstock\", theme: {locale: \"en\"}, webuiAjax: false, webuiAll: true};',
          'resources': [ '../resources/' ]
        }
    }
}
