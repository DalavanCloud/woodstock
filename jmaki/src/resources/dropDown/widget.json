{
    "name": "dropDown",
    "type": "@JS_NAME@",
    "version": "@JS_VERSION@",
    "jmakiVersion": "1.8",
    "description": "Woodstock dropDown widget.",
    "args": [
      {"className": {
         "type": "STRING",
         "description": "CSS selector.",
         "defaultValue": ""}},
      {"dir": {
         "type": "STRING",
         "description": "Specifies the directionality of text.",
         "defaultValue": ""}},
      {"disabled": {
         "type": "BOOLEAN",
         "description": "Disable element.",
         "defaultValue": ""}},
      {"id": {
         "type": "STRING",
         "description": "Uniquely identifies an element within a document.",
         "defaultValue": ""}},
      {"label": {
         "type": "OBJECT",
         "description": "Defines the widget and its properties to use for",
         "defaultValue": ""}},
      {"labelOnTop": {
         "type": "BOOLEAN",
         "description": "If true the label appears above the dropdown.",
         "defaultValue": ""}},
      {"lang": {
         "type": "STRING",
         "description": "Specifies the language of attribute values and content.",
         "defaultValue": ""}},
      {"onBlur": {
         "type": "STRING",
         "description": "Element lost focus.",
         "defaultValue": ""}},
      {"onChange": {
         "type": "STRING",
         "description": "Option selection is changed.",
         "defaultValue": ""}},
      {"onClick": {
         "type": "STRING",
         "description": "Mouse button is clicked on element.",
         "defaultValue": ""}},
      {"onDblClick": {
         "type": "STRING",
         "description": "Mouse button is double-clicked on element.",
         "defaultValue": ""}},
      {"onFocus": {
         "type": "STRING",
         "description": "Element received focus.",
         "defaultValue": ""}},
      {"onKeyDown": {
         "type": "STRING",
         "description": "Key is pressed down over element.",
         "defaultValue": ""}},
      {"onKeyPress": {
         "type": "STRING",
         "description": "Key is pressed and released over element.",
         "defaultValue": ""}},
      {"onKeyUp": {
         "type": "STRING",
         "description": "Key is released over element.",
         "defaultValue": ""}},
      {"onMouseDown": {
         "type": "STRING",
         "description": "Mouse button is pressed over element.",
         "defaultValue": ""}},
      {"onMouseOut": {
         "type": "STRING",
         "description": "Mouse is moved away from element.",
         "defaultValue": ""}},
      {"onMouseOver": {
         "type": "STRING",
         "description": "Mouse is moved onto element.",
         "defaultValue": ""}},
      {"onMouseUp": {
         "type": "STRING",
         "description": "Mouse button is released over element.",
         "defaultValue": ""}},
      {"onMouseMove": {
         "type": "STRING",
         "description": "Mouse is moved while over element.",
         "defaultValue": ""}},
      {"onSelect": {
         "type": "STRING",
         "description": "Option text is highlighted.",
         "defaultValue": ""}},
      {"options": {
         "type": "UNKNOWN",
         "description": "See <code>selectBase._setOptions</code> or",
         "defaultValue": ""}},
      {"size": {
         "type": "INT",
         "description": "The number of option rows to display.",
         "defaultValue": ""}},
      {"style": {
         "type": "STRING",
         "description": "Specify style rules inline.",
         "defaultValue": ""}},
      {"submitForm": {
         "type": "BOOLEAN",
         "description": "If true the dropDown performs like a",
         "defaultValue": ""}},
      {"tabIndex": {
         "type": "INT",
         "description": "Position in tabbing order.",
         "defaultValue": ""}},
      {"title": {
         "type": "STRING",
         "description": "The a HTML title attribue for the <code>select</code> element.",
         "defaultValue": ""}},
      {"visible": {
         "type": "BOOLEAN",
         "description": "Hide or show element.",
         "defaultValue": ""}},
      {"separator": {
         "type": "BOOLEAN",
         "description": "If true, element is a option separator.",
         "defaultValue": ""}},
      {"group": {
         "type": "BOOLEAN",
         "description": "If true the element is a optGroup instance.",
         "defaultValue": ""}},
      {"disabled": {
         "type": "BOOLEAN",
         "description": "If true the element is disabled.",
         "defaultValue": ""}},
      {"selected": {
         "type": "BOOLEAN",
         "description": "If true the element is selected.",
         "defaultValue": ""}}
    ],
    "config": {
      "type": {
        "id": "@JS_NAME@",
        "libs": [
          "@JS_PATH@/bootstrap.js"
        ],
        "resources": [ "../resources/" ]
        }
    },
    "topicInfo": {
        "type": "publish",
        "topicName": "/@JS_NS@/dropDown/onSelect",
        "description": "Published when an option is selected."
    }
}
